
{
  "expo": {
    "name": "PortionTracker",
    "slug": "portion-tracker",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/9b2acb8c-6cd5-475b-8286-4171fe236ec4.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/9b2acb8c-6cd5-475b-8286-4171fe236ec4.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFFFFF"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.portiontracker.app",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      },
      "buildNumber": "1.0.0"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/9b2acb8c-6cd5-475b-8286-4171fe236ec4.png",
        "backgroundColor": "#FFFFFF"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.portiontracker.app",
      "versionCode": 1
    },
    "web": {
      "favicon": "./assets/images/9b2acb8c-6cd5-475b-8286-4171fe236ec4.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-font",
      "expo-router",
      "expo-web-browser",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/9b2acb8c-6cd5-475b-8286-4171fe236ec4.png",
          "color": "#ffffff"
        }
      ]
    ],
    "scheme": "portiontracker",
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "your-project-id-here"
      }
    }
  }
}
